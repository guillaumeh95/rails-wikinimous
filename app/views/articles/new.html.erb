<h1>New article</h1>

<%= form_for(@article) do |f| %>
  <div class="form-group">
    <p><%= f.label :title %></p>
    <p><%= f.text_field :title %></p>
  </div>
  <div class="form-group">
    <p><%= f.label :content %></p>
    <p><%= f.text_field :content %></p>
  </div>
  <div>
    <%= f.submit "Update Article" %>
  </div>
<% end %>
